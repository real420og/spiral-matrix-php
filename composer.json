{
    "type": "project",
    "license": [
        "MIT"
    ],
    "require": {
        "php": "^7.4.0",
        "symfony/console": "^5.2"
    },
    "require-dev": {
        "real420og/php-cs-fixer-config": "^0.0.1",
        "phpunit/phpunit": "^9.5",
        "real420og/phpstan-config": "^0.0.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
